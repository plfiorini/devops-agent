	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Agent",
			"skipFiles": ["<node_internals>/**"],
			"runtimeVersion": "24",
			"runtimeArgs": [
				"--disable-warning=ExperimentalWarning",
				"--experimental-transform-type"
			],
			"program": "${workspaceFolder}/src/index.ts",
			"outFiles": ["${workspaceFolder}/**/*.js"]
		}
	]
}
